	    # look for UTS_RELEASE in a different location (changed in kernel 2.6.18)
	    echo "#include <linux/utsrelease.h>" > hello.c
	    echo "char *jsver=UTS_RELEASE" >> hello.c
            GCC_OUTPUT=`gcc -E hello.c $INCLUDEDIRS`
