XUSB_PRODUCT="3fd/7"
XUSB_PRESENT=`echo $PRODUCT | grep -c $XUSB_PRODUCT`
if [ $XUSB_PRESENT = "1" ]
then
	/sbin/fxload -v -t fx2 -I usb/xusbdfwu.fw/xusbdfwu.hex
fi;

XUSB_PRODUCT="3fd/9"
XUSB_PRESENT=`echo $PRODUCT | grep -c $XUSB_PRODUCT`
if [ $XUSB_PRESENT = "1" ]
then
	/sbin/fxload -v -t fx2 -I usb/xusbdfwu.fw/xusb_xup.hex
fi;

XUSB_PRODUCT="3fd/d"
XUSB_PRESENT=`echo $PRODUCT | grep -c $XUSB_PRODUCT`
if [ $XUSB_PRESENT = "1" ]
then
	/sbin/fxload -v -t fx2 -I usb/xusbdfwu.fw/xusb_emb.hex
fi;

XUSB_PRODUCT="3fd/f"
XUSB_PRESENT=`echo $PRODUCT | grep -c $XUSB_PRODUCT`
if [ $XUSB_PRESENT = "1" ]
then
	/sbin/fxload -v -t fx2 -I usb/xusbdfwu.fw/xusb_xlp.hex
fi;

XUSB_PRODUCT="3fd/13"
XUSB_PRESENT=`echo $PRODUCT | grep -c $XUSB_PRODUCT`
if [ $XUSB_PRESENT = "1" ]
then
	/sbin/fxload -v -t fx2 -I usb/xusbdfwu.fw/xusb_xpr.hex
fi;

XUSB_PRODUCT="3fd/8"
XUSB_PRESENT=`echo $PRODUCT | grep -c $XUSB_PRODUCT`
if [ $XUSB_PRESENT = "1" ]
then
	chmod 666 $DEVICE
fi;
#End of file.
