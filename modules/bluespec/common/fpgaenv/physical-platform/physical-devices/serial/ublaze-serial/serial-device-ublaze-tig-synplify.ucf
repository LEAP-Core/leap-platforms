############################################################################
# Ignore Timing Violations at Sync FIFOs                                   #
############################################################################

## Un-comment these lines if you see timing violations at Sync FIFOs that
## you're sure wouldn't cause any problems

# SyncFIFO: receivefifo

NET "m_llpi_phys_plat_serial_device_receivefifo$dD_OUT[*]"     TIG;
NET "m_llpi_phys_plat_serial_device_receivefifo/dGDeqPtr[*]"   TIG;
NET "m_llpi_phys_plat_serial_device_receivefifo/dGDeqPtr1[*]"  TIG;
NET "m_llpi_phys_plat_serial_device_receivefifo/sGEnqPtr[*]"   TIG;
NET "m_llpi_phys_plat_serial_device_receivefifo/sGEnqPtr1[*]"  TIG;


# SyncFIFO: sendfifo

NET "m_llpi_phys_plat_serial_device_sendfifo$dD_OUT[*]"      TIG;
NET "m_llpi_phys_plat_serial_device_sendfifo/dGDeqPtr[*]"    TIG;
NET "m_llpi_phys_plat_serial_device_sendfifo/dGDeqPtr1[*]"   TIG;
NET "m_llpi_phys_plat_serial_device_sendfifo/sGEnqPtr[*]"    TIG;
NET "m_llpi_phys_plat_serial_device_sendfifo/sGEnqPtr1[*]"   TIG;
