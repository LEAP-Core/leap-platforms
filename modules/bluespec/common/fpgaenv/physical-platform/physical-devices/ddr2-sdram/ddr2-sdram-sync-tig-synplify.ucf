NET "*_ddr2_sdram_device_*_syncRequestQ/dGDeqPtr1[*]"   TIG;
NET "*_ddr2_sdram_device_*_syncRequestQ/sGEnqPtr[*]"    TIG;
NET "*_ddr2_sdram_device_*_syncRequestQ/sGEnqPtr1[*]"   TIG;

# SyncFIFOLevel: syncWriteDataQ

#NET "*_ddr2_sdram_device_*_syncWriteDataQ_dD_OUT[*]"    TIG;
